		<!-- <plugin type="org.mybatis.generator.SelectByPagePlugin" /> -->
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
			<property name="suppressDate" value="false" />
		</commentGenerator>
		<!-- 此处配置 需要生成代码的数据库 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://47.94.90.165:3306/carmanager?useUnicode=true&amp;characterEncoding=UTF-8"
			userId="root" password="root" />
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		<!-- 此处配置生成model实体文件 -->
		<javaModelGenerator targetPackage="com.ruizhua.paw.model"
			targetProject="G:\workspace\paw\src\main\java">
			<property name="enableSubPackages" value="false" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<!-- 此处配置生成mapper.xml文件 如果原有mapper.xml文件中有自定义接口 此处需要注释掉 或者将自定义接口进行备份 -->
		<sqlMapGenerator targetPackage="com.ruizhua.paw.mapper"
			targetProject="G:\workspace\paw\src\main\java">
			<property name="enableSubPackages" value="false" />
		</sqlMapGenerator>
		<!-- 配置生成DAO层代码 若不需要生成DAO层代码或者DAO存在自定义的方法 为防止被覆盖掉 此处需要注释 -->
		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.ruizhua.paw.dao" targetProject="G:\workspace\paw\src\main\java">
			<property name="enableSubPackages" value="false" />
		</javaClientGenerator>

